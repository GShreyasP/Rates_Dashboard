{
  "version": 2,
  "buildCommand": "cd Frontend/frontend && npm install && npm run build && cd ../.. && mkdir -p public && cp -r Frontend/frontend/dist/* public/",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/test",
      "destination": "/api/test.py"
    },
    {
      "source": "/api/macro",
      "destination": "/api/macro.py"
    },
    {
      "source": "/api/rates",
      "destination": "/api/rates.py"
    },
    {
      "source": "/api/fedwatch",
      "destination": "/api/fedwatch.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
